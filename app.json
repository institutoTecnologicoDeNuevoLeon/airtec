{
  "expo": {
    "name": "Airtec",
    "slug": "clima-04",
    "scheme": "climaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconosClima/aire-limpio.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/iconosClima/aire-limpio.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconosClima/aire-limpio.png",
        "backgroundColor": "#34a0a4"
      },
      "package": "com.milo940707.clima04",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "mailto"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "geo"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/iconosClima/luna-llena.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconosClima/aire-limpio.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/iconosClima/aire-limpio.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa9d4040-8189-4cf5-850d-b351803de433"
      }
    }
  }
}

