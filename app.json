{
  "expo": {
    "name": "Airtec",
    "slug": "clima-04",
    "scheme": "climaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconosClima/aire-limpio.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/iconosClima/aire-limpio.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconosClima/aire-limpio.png",
        "backgroundColor": "#34a0a4"
      },
      "package": "com.airtec.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "mailto"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "geo"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/images/iconosClima/luna-llena.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconosClima/aire-limpio.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/iconosClima/aire-limpio.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3178ab1a-bf16-40c2-8071-d54d483dd0c7"
      }
    },
    "owner": "tecnologico_de_nuevo_leon",
    "updates": {
      "url": "https://u.expo.dev/3178ab1a-bf16-40c2-8071-d54d483dd0c7"
    }
  }
}
